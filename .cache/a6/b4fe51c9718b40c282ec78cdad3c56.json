 hsl(217, 54%, 11%);\n$dark-blue-card: hsl(216, 50%, 16%);\n$dark-blue-line: hsl(215, 32%, 27%);\n\n$theme-color: (\n    \"primary\": $dark-blue-main,\n    \"card-bg\": $dark-blue-card,\n    \"line\": $dark-blue-line,\n    \"important-color\": white,\n    \"text-color\": $soft-blue\n);\n\n$border-radius: 8px;\n\n* {\n    box-sizing: border-box;\n}\n\nbody {\n    display: flex;\n    align-items: center;\n    justify-content: space-between;\n    flex-direction: column;\n    font-family: 'Outfit', sans-serif;\n    background: map-get($theme-color, \"primary\");\n}\n\n.card {\n    max-width: 290px;\n    font-size: 1.125em;\n    color: map-get($theme-color, \"text-color\");\n    background-color: map-get($theme-color, \"card-bg\");\n    padding: 1em;\n    border-radius: $border-radius;\n    box-shadow: 1px 1px 15px darken(map-get($theme-color, \"primary\"), 5);\n}\n\n.nft-data {\n    display: flex;\n    flex-direction: column;\n    gap: 0.8em;\n    margin-bottom: 1em;\n}\n\n.card-cover {\n    width: 100%;\n    border-radius: $border-radius;\n}\n\n.card-title {\n    color: map-get($theme-color, \"important-color\");\n    margin: 0 0 0 0;\n}\n\n.card-text {\n    margin: 0 0;\n}\n\n.card-info {\n    display: flex;\n    justify-content: space-between;\n    flex-shrink: 1;\n    font-size: smaller;\n\n    .card-price {\n        display: flex;\n        align-items: center;\n        gap: 0.3em;\n        color: $cyan;\n        font-weight: bold;\n\n        img {\n            width: 8px;\n        }\n    }\n\n    .card-time {\n        display: flex;\n        align-items: center;\n        gap: 0.5em;\n\n        img {\n            width: 12px;\n        }\n    }\n}\n\n.card-author {\n    display: flex;\n    align-items: center;\n    gap: 0.8em;\n    font-size: smaller;\n\n    img {\n        width: 24px;\n        border: 1px solid white;\n        border-radius: 50%;\n    }\n}\n\n.author-name {\n    color: map-get($theme-color, \"important-color\");\n}\n\nhr {\n    width: 100%;\n    border: none;\n    border-top: 1px solid map-get($theme-color, \"line\");\n}"},"lineCount":null}},"error":null,"hash":"651346c6dd0a9325c3e4b268451e0e72","cacheData":{"env":{}}}